    "publish": "vsce publish"
  },
  "galleryBanner": {
    "color": "#2D3A52",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "grand-budapest-icon.jpg",
  "contributes": {
    "themes": [
      {
        "label": "Grand Budapest Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/grand-budapest-dark.json"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^1.14.12",
    "vsce": "^1.103.1"
  }
