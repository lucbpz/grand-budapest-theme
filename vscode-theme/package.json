{
  "name": "grand-budapest-theme",
  "displayName": "Grand Budapest",
  "description": "A colorful theme inspired by the film Grand Budapest Hotel by Wes Anderson",
  "version": "1.0.4",
  "publisher": "thislucas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucbpz/grand-budapest-theme"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Grand Budapest",
    "Grand Budapest Dark"
  ],
  "scripts": {
